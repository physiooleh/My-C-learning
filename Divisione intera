#include <iostream>
using namespace std;

int main() {
int numerators[] = {7,14, 9, 20, 17};
int denominators[] = {3, 4, 2, 6, 5};
int n = 5;

for (int i = 0; i < n; i++) {
int a = numerators[i];
int b = denominators [i];
int quotient = a / b; //cile czyslo
int remainder = a % b; //ostacza

cout << a << "/" << b << "=" << quotient
<< ", ostacza = " << remainder << endl;
}
return 0;
}
